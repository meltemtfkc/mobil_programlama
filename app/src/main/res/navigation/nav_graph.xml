<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.meltemtufekci.myproject.ui.ListFragment"
        android:label="Kullanıcılar"
        tools:layout="@layout/fragment_list">
        <action
            android:id="@+id/action_listFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.meltemtufekci.myproject.ui.DetailFragment"
        android:label="Detaylar"
        tools:layout="@layout/fragment_detail">
        <argument
            android:name="userItemDetail"
            app:argType="com.meltemtufekci.myproject.data.User" />
    </fragment>
</navigation>